                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "board_title": {
                                        "description": "게시글 제목",
                                        "type": "string",
                                        "example": "공지사항입니다."
                                    },
                                    "board_content": {
                                        "description": "게시글 내용",
                                        "type": "string",
                                        "example": "공지사항 내용입니다."
                                    }
                                },
                                "type": "object"
